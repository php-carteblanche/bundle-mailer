{
    "name": "carte-blanche/bundle-mailer",
    "type": "carte-blanche-bundle",
    "description": "A bundle to send mass MIME emails",
    "keywords": [ "carte blanche", "bundles" ],
    "homepage": "http://github.com/php-carteblanche/bundle-mailer",
    "license": "Apache-2.0",
    "authors": [{
        "name": "Les Ateliers Pierrot & contributors",
        "email": "contact@ateliers-pierrot.fr",
        "homepage": "http://www.ateliers-pierrot.fr/"
    }],
    "require": {
        "atelierspierrot/mime-mailer": "1.*",
        "carte-blanche/core": "dev-cb_wip"
    },
    "autoload": { 
        "psr-0": { "MimeEmail": "src" },
        "classmap": [ "src/MimeEmail/Controller" ]
    },
    "repositories": [{
        "type": "composer", "url": "http://packagist.ateliers-pierrot.fr/"
    }],
    "extra": {
        "views": "src/MimeEmail/views",
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
